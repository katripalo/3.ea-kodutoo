{
	"manifest_version":2,
    "name": "Time saver",
    "description": "first chrome extension",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
		"default_popup": "popup.html"
    },
	"icons" : {
	"64" : "icon.png"
	},	
	  "content_scripts": [
	  {
	   "matches": ["https://9gag.com/","https://www.facebook.com/" ],
	   "js": ["popup.js","myScript.js"]
	  }
	],
	"permissions": [
	   "notifications"
	]

}